Map {
  background-color: #dadaeb;
}


#nodes[zoom=2]{
  [degree >= 1] { marker-width:1; marker-fill: #ffffcc; }
  [degree >= 3] { marker-width:10; marker-fill:#c7e9b4 ;}
  [degree >= 4]   { marker-width:12; marker-fill:#41b6c4 ; }
  [degree >= 5] { marker-width:14; marker-fill: #7fcdbb; }
  [degree >= 6]   { marker-width:18; marker-fill: #41b6c4; }
  [degree >= 7] { marker-width:22; marker-fill: #2c7fb8; }
  [degree >= 8]   { marker-width:26; marker-fill: #253494; }
  marker-line-color:#6c0321;
  marker-allow-overlap:true;
}

